          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH time_buckets AS (\r\n    SELECT date_trunc('minute', time) AS time,\r\n           table_name,\r\n           AVG(wait_latency) AS mean_wait_latency\r\n    FROM table_io\r\n    WHERE $__timeFilter(time)\r\n      AND db = 'master'\r\n    GROUP BY time, table_name\r\n),\r\nlag_values AS (\r\n    SELECT time,\r\n           table_name,\r\n           mean_wait_latency,\r\n           LAG(mean_wait_latency) OVER (PARTITION BY table_name ORDER BY time) AS prev_mean_wait_latency\r\n    FROM time_buckets\r\n),\r\ntable_latencies AS (\r\n    SELECT table_name,\r\n           SUM(mean_wait_latency - prev_mean_wait_latency) AS total_wait_latency_diff\r\n    FROM lag_values\r\n    WHERE mean_wait_latency - prev_mean_wait_latency IS NOT NULL\r\n    GROUP BY table_name\r\n),\r\ntotal_latency AS (\r\n    SELECT SUM(total_wait_latency_diff) AS overall_total_diff\r\n    FROM table_latencies\r\n),\r\ntable_with_percentage AS (\r\n    SELECT table_name,\r\n           total_wait_latency_diff,\r\n           (total_wait_latency_diff / (SELECT overall_total_diff FROM total_latency)) * 100 AS percentage_of_total\r\n    FROM table_latencies\r\n),\r\ntable_with_cumulative_percentage AS (\r\n    SELECT table_name,\r\n           percentage_of_total,\r\n           SUM(percentage_of_total) OVER (ORDER BY percentage_of_total DESC) AS cumulative_percentage\r\n    FROM table_with_percentage\r\n)\r\nSELECT\r\n    CASE\r\n        WHEN cumulative_percentage - percentage_of_total > 99 THEN 'OTHER'\r\n        ELSE table_name\r\n    END AS table_name,\r\n    SUM(percentage_of_total) AS percentage_of_total\r\nFROM table_with_cumulative_percentage\r\nGROUP BY CASE\r\n            WHEN cumulative_percentage - percentage_of_total > 99 THEN 'OTHER'\r\n            ELSE table_name\r\n         END\r\nORDER BY percentage_of_total DESC;",
